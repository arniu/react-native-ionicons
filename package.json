{
  "name": "react-native-ionicons",
  "description": "Ionic icons for React Native",
  "version": "4.6.4",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "prepublishOnly": "npm test",
    "generate": "node generate.js",
    "prettier": "prettier --write '**/*.{js,json,ts,md}'",
    "test": "npm run generate && jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arniu/react-native-ionicons.git"
  },
  "keywords": [
    "react-native",
    "ionicons",
    "ionic",
    "icons"
  ],
  "author": "Arniu Tseng",
  "license": "MIT",
  "homepage": "https://github.com/arniu/react-native-ionicons#readme",
  "bugs": {
    "url": "https://github.com/arniu/react-native-ionicons/issues"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "babel-jest": "^25.1.0",
    "husky": "^4.2.3",
    "ionicons": "^4.6.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  },
  "rnpm": {
    "assets": [
      "fonts"
    ]
  }
}
